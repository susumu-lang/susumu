{
  "Arrow Flow": {
    "prefix": "flow",
    "body": [
      "${1:input} -> ${2:function} -> ${3:output}"
    ],
    "description": "Basic arrow flow pattern"
  },
  "Function Definition": {
    "prefix": "fn",
    "body": [
      "${1:functionName}(${2:param}) {",
      "    ${2:param} -> ${3:operation} -> return",
      "}"
    ],
    "description": "Function definition with arrow flow"
  },
  "Conditional Flow": {
    "prefix": "if",
    "body": [
      "${1:value} -> i ${2:condition} {",
      "    ${3:then_branch}",
      "} e {",
      "    ${4:else_branch}",
      "}"
    ],
    "description": "Conditional with arrow flow"
  },
  "Else-If Chain": {
    "prefix": "elif",
    "body": [
      "${1:value} -> i ${2:condition1} {",
      "    ${3:branch1}",
      "} ei ${4:condition2} {",
      "    ${5:branch2}",
      "} e {",
      "    ${6:else_branch}",
      "}"
    ],
    "description": "Else-if chain pattern"
  },
  "Trace Annotation": {
    "prefix": "@trace",
    "body": [
      "@trace <- \"${1:trace-name}\""
    ],
    "description": "Trace annotation for debugging"
  },
  "Monitor Annotation": {
    "prefix": "@monitor",
    "body": [
      "@monitor <- [\"${1:metric1}\", \"${2:metric2}\"]"
    ],
    "description": "Monitor annotation for metrics"
  },
  "Config Annotation": {
    "prefix": "@config",
    "body": [
      "@config <- {",
      "    ${1:key}: \"${2:value}\",",
      "    ${3:timeout}: \"${4:30s}\"",
      "}"
    ],
    "description": "Configuration annotation"
  },
  "Parallel Annotation": {
    "prefix": "@parallel",
    "body": [
      "@parallel"
    ],
    "description": "Parallel processing annotation"
  },
  "Debug Annotation": {
    "prefix": "@debug",
    "body": [
      "@debug <- \"${1:debug-context}\""
    ],
    "description": "Debug annotation"
  },
  "Data Pipeline": {
    "prefix": "pipeline",
    "body": [
      "${1:data} -> validate -> i valid {",
      "    ${1:data} -> transform -> process -> save",
      "} e {",
      "    ${1:data} -> logError -> retry",
      "}"
    ],
    "description": "Common data processing pipeline"
  },
  "Error Handling": {
    "prefix": "error",
    "body": [
      "${1:operation} -> i success {",
      "    result -> return <- result",
      "} e {",
      "    error <- \"${2:error_message}\"",
      "}"
    ],
    "description": "Error handling pattern"
  },
  "Main Function": {
    "prefix": "main",
    "body": [
      "main() {",
      "    ${1:// Your code here}",
      "}"
    ],
    "description": "Main function entry point"
  },
  "Pattern Matching": {
    "prefix": "match",
    "body": [
      "${1:value} -> match {",
      "    some <- ${2:pattern} -> ${3:action},",
      "    none -> ${4:default_action}",
      "}"
    ],
    "description": "Pattern matching structure"
  },
  "Bidirectional Flow": {
    "prefix": "biflow",
    "body": [
      "${1:source1} -> ${2:function} <- ${3:source2} -> ${4:output}"
    ],
    "description": "Bidirectional arrow flow"
  }
}